<h1>Listing old_games</h1>

<table>
  <tr>
    <th>Game</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @old_games.each do |old_game| %>
  <tr>
    <td><%= old_game.game_id %></td>
    <td><%= link_to 'Show', old_game %></td>
    <td><%= link_to 'Edit', edit_old_game_path(old_game) %></td>
    <td><%= link_to 'Destroy', old_game, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Old game', new_old_game_path %>
